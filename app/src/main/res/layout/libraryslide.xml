<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/FrameLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginRight="80dp"
    android:background="#e9e9e9" >

    <LinearLayout
        android:id="@+id/slide_menu_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        
      <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingTop="25dp"
            android:paddingBottom="10dp"
            android:gravity="center"
            android:background="@drawable/menubar">

          <FrameLayout
              android:layout_width="wrap_content"
              android:layout_height="wrap_content">
            
           <ImageView
            android:id="@+id/imageview_logo_slider"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_gravity="center"
            android:scaleType="fitXY" />

          <ProgressBar
              android:id="@+id/homeprogress_slider"
              style="?android:attr/progressBarStyle"
              android:layout_width="wrap_content"
              android:indeterminateTint="#D81B60"
              android:layout_gravity="center"
              android:layout_height="wrap_content"/>

          </FrameLayout>

          <TextView
              android:id="@+id/textview_empname"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:textColor="#ffffff"
              android:paddingTop="5dp"
              android:paddingBottom="10dp"
              android:layout_gravity="center"
              android:textAppearance="@style/Base.TextAppearance.AppCompat.Medium"/>
          <TextView
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:layout_gravity="bottom|right"
              android:gravity="right"
              android:textColor="#ffffff"
              android:text="Version 1.0.1"
              android:paddingRight="8dp"
              android:textAppearance="@style/Base.TextAppearance.AppCompat.Small" />
           

        </LinearLayout>
        
 
        <View
		    android:layout_width="match_parent"
		    android:layout_height="1dip"
		    android:background="#575757"
            android:layout_marginBottom="15dp"/>

        <androidx.core.widget.NestedScrollView
            android:id="@+id/scroll_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:overScrollMode="never"
            android:fillViewport="true">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:orientation="vertical">

           <LinearLayout
               android:id="@+id/lin_upprofile"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_marginLeft="30dp"
               android:orientation="horizontal">
           
         <ImageView
            android:id="@+id/imageView1"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_gravity="left|center"
            android:src="@drawable/updateprofile"
            android:tint="#267be6"
            android:padding="5dp"
            android:scaleType="fitXY" />          
        
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Update Profile"
            android:padding="17dp"
            android:textColor="#000000"
            android:textAppearance="?android:attr/textAppearanceMedium" />

       
        
         </LinearLayout>

       <View
           android:layout_width="match_parent"
           android:layout_height="1dip"
           android:background="#575757"/>
		    
         <LinearLayout
               android:id="@+id/lin_holiday_list"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:orientation="horizontal"
               android:layout_marginLeft="30dp">
        
           <ImageView
            android:id="@+id/imageView2"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_gravity="left|center"
            android:src="@drawable/holidaylist"
            android:tint="#267be6"
            android:padding="5dp"
            android:scaleType="fitXY" /> 

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Holiday List"
            android:textColor="#000000"
            android:padding="17dp"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        
         </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:background="#575757"/>
		    
