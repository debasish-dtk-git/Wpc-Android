                android:id="@+id/imageView42"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_gravity="left|center"
                android:src="@drawable/dailyattendance"
                android:tint="#267be6"
                android:padding="5dp"
                android:scaleType="fitXY" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Attendance Sheet"
                android:padding="17dp"
                android:textColor="#000000"
                android:textAppearance="?android:attr/textAppearanceMedium" />

        </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:background="#575757"/>

                <LinearLayout
                    android:id="@+id/lin_contact_aggrement"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:orientation="horizontal" >

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_gravity="left|center"
                        android:src="@drawable/agreement"
                        android:tint="#267be6"
                        android:padding="5dp"
                        android:scaleType="fitXY" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Contract Agreement"
                        android:padding="17dp"
                        android:textColor="#000000"
                        android:textAppearance="?android:attr/textAppearanceMedium" />

                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:background="#575757"/>

                <LinearLayout
                    android:id="@+id/lin_change_of_circumstnce"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:orientation="horizontal" >

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_gravity="left|center"
                        android:src="@drawable/coc"
                        android:tint="#267be6"
                        android:padding="5dp"
                        android:scaleType="fitXY" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Change of Circumstances"
                        android:padding="17dp"
                        android:textColor="#000000"
                        android:textAppearance="?android:attr/textAppearanceMedium" />

                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:background="#575757"/>

                <LinearLayout
                    android:id="@+id/lin_messg_centre_emp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:orientation="horizontal">

                        <ImageView
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_gravity="center"
                            android:src="@drawable/message_center_icon"
                            android:tint="#267be6"
                            android:padding="5dp"
                            android:scaleType="fitXY" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Message Centre"
                        android:padding="17dp"
                        android:textColor="#000000"
                        android:textAppearance="?android:attr/textAppearanceMedium" />



                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:background="#575757"/>


         <LinearLayout
            android:id="@+id/support"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_gravity="left|center"
                android:src="@drawable/support"
                android:tint="#267be6"
                android:padding="5dp"
                android:scaleType="fitXY" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Support"
                android:padding="17dp"
                android:textColor="#000000"
                android:textAppearance="?android:attr/textAppearanceMedium" />

        </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:background="#575757"/>

                <LinearLayout
                    android:id="@+id/logout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:orientation="horizontal" >

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_gravity="left|center"
                        android:src="@drawable/logout_icn"
                        android:tint="#267be6"
                        android:padding="5dp"
                        android:scaleType="fitXY" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
